{
    "manifest_version": 1,
    "name": "GruzAuth | Gruzservices",
    "short_name": "GruzAuth",
    "description": "GruzAuth securly authenticate all of your websites.",
    "version": "1.1",
    "icons": [
        {
          "src": "icons2/manifest-icon-192.maskable.png",
          "sizes": "192x192",
          "type": "image/png",
          "purpose": "any"
        },
        {
          "src": "icons2/manifest-icon-192.maskable.png",
          "sizes": "192x192",
          "type": "image/png",
          "purpose": "maskable"
        },
        {
          "src": "icons2/manifest-icon-512.maskable.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "any"
        },
        {
          "src": "icons2/manifest-icon-512.maskable.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "maskable"
        }
      ],
    "start_url": "/",
    "display_override": ["window-controls-overlay"],
    "theme_color": "rgb(28, 41, 56)",
    "background-color": "#000000",
    "background_color": "#000000",
    "display": "fullscreen",
    "orientation": "portrait"
}
